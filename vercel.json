{
    "rewrites": [
        {
            "source": "/((?!api/).*)",
            "destination": "/"
        }
    ],
    "functions": {
        "api/*.js": {
            "memory": 256,
            "maxDuration": 10
        },
        "api/*.php": {
            "runtime": "vercel-php@0.7.2"
        }
    }
}